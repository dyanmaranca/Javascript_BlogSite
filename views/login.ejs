<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Login</title>
    <link rel="stylesheet" href="css/normalizedm.css">
    <link rel="stylesheet" href="css/style.css">
    <script src="https://cdn.tiny.cloud/1/vle74anow2fmw5nkg95d2ap0u6uawq6jfmm3paygeu5fbbo4/tinymce/7/tinymce.min.js" referrerpolicy="origin"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
</head>
<body>
    <div class="container">
    <!-- Add header and navigation -->
    <%- include ('common/header.ejs') %>

    <p class="adminlogin"> <a href="">Login</a></p>

    <div class="navmain">
        <%- include ('common/navigation posts.ejs') %>
        <main>
            <form class="loginform" method="post" action="/adminwelcome">
                <h3>Login</h3>
                <% if (typeof (msg) != 'undefined'){%>
                    <p class="loginerror"> <%=msg %> </p>
                <%}%>
                <label for="username">Username</label>
                <input type="text" name="username"><br>
                <label for="password">Password</label>
                <input type="password" name="password"><br>
                <input class="login" type="submit" value="Submit">
            </form>
        </main>
    </div>
    <!-- Add footer -->
    <%- include ('common/footer.ejs') %>
    </div>
</body>
</html>